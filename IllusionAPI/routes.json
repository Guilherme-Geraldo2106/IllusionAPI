{
  "illusionRouteConfig": {
    "illusionRoutes": [
      {
        "method": "GET",
        "path": "/posts",
        "response": {
          "status": 200,
          "body": [
            {
              "id": 1,
              "title": "Post 1",
              "content": "This is the first post."
            },
            {
              "id": 2,
              "title": "Post 2",
              "content": "This is the second post."
            }
          ]
        }
      },
      {
        "method": "GET",
        "path": "/posts/{id}",
        "response": {
          "status": 200,
          "body": {
            "id": "{id}",
            "title": "Post {id}",
            "content": "This is the content of post {id}."
          }
        }
      },
      {
        "method": "GET",
        "path": "/login",
        "response": {
          "status": 200,
          "body": "Authorized"
        }
      }
    ]
  }
}
